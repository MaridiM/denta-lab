    "@babel/preset-env": "^7.13.0",
    "babel-plugin-import-graphql": "^2.8.1",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "apollo-server-express": "^2.21.0",
